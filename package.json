{
  "name": "@seolhun/kuery",
  "version": "0.1.0",
  "author": "Seolhun",
  "license": "MIT",
  "description": "To build key and value Frontend Query Builder like MongoDB",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "prebuild": "rm -rf ./dist",
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc",
    "build:esm": "tsc --target ESNext --module ES6 --outDir esm",
    "test": "jest --coverage",
    "test:type": "tsc --noEmit",
    "prettier": "prettier --write \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,test,examples}/**/*.{ts,tsx}\" --fix"
  },
  "files": [
    "dist/**",
    "esm/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seolhun/kuery.git"
  },
  "keywords": [
    "kuery",
    "typescript",
    "key",
    "value"
  ],
  "bugs": {
    "url": "https://github.com/Seolhun/kuery/issues"
  },
  "homepage": "https://github.com/Seolhun/kuery#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-eslint-plugin": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^28.1.3",
    "prettier": "^2.8.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@fxts/core": "^0.9.0"
  }
}
