{
  "name": "@seolhun/kuery",
  "version": "0.1.1",
  "author": "Seolhun",
  "license": "MIT",
  "description": "To parse/stringify query string to improve ui/ux",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "prebuild": "rm -rf ./dist",
    "build": "tsup",
    "test": "vitest run --coverage",
    "test:type": "tsc --noEmit",
    "prettier": "prettier --write \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,test,examples}/**/*.{ts,tsx}\" --fix"
  },
  "files": [
    "dist/**",
    "esm/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seolhun/kuery.git"
  },
  "keywords": [
    "kuery",
    "search",
    "param",
    "url"
  ],
  "bugs": {
    "url": "https://github.com/Seolhun/kuery/issues"
  },
  "homepage": "https://github.com/Seolhun/kuery#readme",
  "devDependencies": {
    "@seolhun/eslint-config": "^1.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "4",
    "@typescript-eslint/parser": "4",
    "@vitest/coverage-istanbul": "^1.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8",
    "eslint-plugin-eslint-plugin": "3",
    "eslint-plugin-import": "2",
    "eslint-plugin-jsx-a11y": "6",
    "eslint-plugin-node": "11",
    "eslint-plugin-prettier": "3",
    "eslint-plugin-react": "7",
    "eslint-plugin-react-hooks": "4",
    "jsdom": "^24.1.0",
    "prettier": "^2.7.1",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  }
}
